{
  "name": "@wobble/website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./trpc": "./src/routes/trpc/index.ts"
  },
  "imports": {
    "#/*": "./src/*",
    "#pages/*": "./src/pages/*",
    "#modules/*": "./src/modules/*",
    "#lib/*": "./src/lib/*",
    "#routes/*": "./src/routes/*",
    "#middleware/*": "./src/middleware/*"
  },
  "scripts": {
    "dev": "bun --hot --define EXECUTABLE=false src/index.ts",
    "start": "NODE_ENV=production bun --define EXECUTABLE=false src/index.ts",
    "build": "bun run build.ts",
    "db:push": "bun drizzle-kit push",
    "db:generate": "bun drizzle-kit generate",
    "db:migrate": "bun drizzle-kit migrate",
    "db:studio": "bun drizzle-kit studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@electric-sql/pglite": "^0.3.14",
    "@hono/trpc-server": "^0.4.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-form": "^1.25.0",
    "@tanstack/react-query": "^5.90.9",
    "@trpc/client": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@trpc/tanstack-react-query": "^11.7.1",
    "@wobble/bot": "workspace:*",
    "arctic": "^3.7.0",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "discord-api-types": "^0.38.36",
    "discord.js": "^14.25.1",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.10.5",
    "ky": "^1.14.0",
    "lucide-react": "^0.555.0",
    "next-themes": "^0.4.6",
    "react": "^19",
    "react-dom": "^19",
    "socket.io": "^4.8.1",
    "sonner": "^2.0.7",
    "superjson": "^2.2.5",
    "tailwind-merge": "^3.4.0",
    "unzipper": "^0.12.3",
    "wouter": "^3.7.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/unzipper": "^0.10.11",
    "drizzle-kit": "^0.31.7",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
